# Fingerprint Data Files
/data/system/sunwave(/.*)?                                                                                    u:object_r:fingerprintd_data_file:s0

# Fingerprint Devices
/dev/fpsensor                                                                                                 u:object_r:fingerprint_device:s0
/dev/madev0                                                                                                   u:object_r:fingerprint_device:s0
/dev/sunwave_fp                                                                                               u:object_r:fingerprint_device:s0

# Fingerprint HAL
/vendor/lib(64)?/hw/microarray\.fingerprint\.default\.so                                                      u:object_r:hal_fingerprint_default_exec:s0
/vendor/lib(64)?/hw/swfingerprint\.default\.so                                                                u:object_r:hal_fingerprint_default_exec:s0

# Light HAL
/(vendor|system/vendor)/bin/hw/android\.hardware\.light@2\.0-service\.D                                    u:object_r:hal_light_default_exec:s0

# Perf HAL
/vendor/lib64/vendor\.qti\.hardware\.perf@2\.0\.so                                                            u:object_r:same_process_hal_file:s0
